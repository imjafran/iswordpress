{
  "name": "Is WP",
  "description": "isWP scans themes and plugins installed on a WordPress website even it's headless, SaaS or BaaS.",
  "version": "0.0.2",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "webRequest"
  ],
  "action": {
    "default_popup": "app.html",
    "default_icon": {
      "16": "/images/iswp-16.png",
      "32": "/images/iswp-32.png",
      "48": "/images/iswp-64.png",
      "128": "/images/iswp.png"
    }
  },
  "icons": {
    "16": "/images/iswp-16.png",
    "32": "/images/iswp-32.png",
    "48": "/images/iswp-64.png",
    "128": "/images/iswp.png"
  }
}
