    "options:tailwind": "npx tailwindcss -i ./src/css/options.css -c ./tailwind.options.js -o ./build/css/options.min.css --watch --minify",
    "popup:webpack": "npx webpack --config ./webpack.popup.js --watch",
    "options:webpack": "npx webpack --config ./webpack.options.js --watch"
